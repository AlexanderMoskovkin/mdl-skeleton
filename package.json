{
  "version": "0.7.6",
  "name": "mdl-skeleton",
  "homepage": "https://techpush.xyz",
  "description": "Material Design Lite - Skeleton for Node.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/mdl-skeleton"
  },
  "author": "@ndaidong",
  "license": "MIT",
  "engines": {
    "node": ">= 7.0"
  },
  "main": "./server.js",
  "scripts": {
    "setup": "node ./bin/setup",
    "start": "DEBUG=*:* node --harmony-async-await server",
    "reset": "node ./bin/reset",
    "lint": "eslint .",
    "test": "testcafe all tests/"
  },
  "dependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-transform-remove-strict-mode": "0.x.x",
    "babel-preset-env": "0.x.x",
    "bellajs": "6.x.x",
    "copy-dir": "0.x.x",
    "css-mqpacker": "5.x.x",
    "cssnano": "3.x.x",
    "debug": "2.x.x",
    "handlebars": "4.x.x",
    "imagemin": "5.x.x",
    "imagemin-mozjpeg": "6.x.x",
    "imagemin-pngquant": "5.x.x",
    "koa": "next",
    "koa-bodyparser": "next",
    "koa-favicon": "next",
    "koa-router": "next",
    "koa-static": "next",
    "mkdirp": "0.x.x",
    "postcss": "5.x.x",
    "postcss-cssnext": "2.x.x",
    "postcss-filter-plugins": "2.x.x",
    "postcss-nesting": "^2.3.1",
    "promise-wtf": "1.x.x",
    "recursive-readdir": "2.x.x",
    "shift-codegen": "5.x.x",
    "shift-parser": "5.x.x",
    "svgo": "0.x.x"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "testcafe": "latest"
  },
  "builder": {
    "directories": [
      "storage/cache",
      "storage/tmp"
    ],
    "cssDir": "assets/css",
    "jsDir": "assets/js",
    "imgDir": "assets/images",
    "fontDir": "assets/fonts",
    "distDir": "dist",
    "vendorDir": "vendor",
    "css": {
      "mdl": "https://code.getmdl.io/1.2.1/material.indigo-pink.min.css"
    },
    "javascript": {
      "bella": "https://cdn.rawgit.com/ndaidong/bella.js/master/dist/bella.js",
      "material": "https://storage.googleapis.com/code.getmdl.io/1.2.1/material.js",
      "doc": "https://raw.githubusercontent.com/ndaidong/doc/master/dist/doc.js"
    }
  }
}
