{
  "version": "0.7.6",
  "name": "mdl-skeleton",
  "homepage": "https://techpush.xyz",
  "description": "Material Design Lite - Skeleton for Node.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/mdl-skeleton"
  },
  "author": "@ndaidong",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0"
  },
  "main": "./server.js",
  "scripts": {
    "setup": "node ./bin/setup",
    "start": "npm run setup && node server",
    "reset": "node ./bin/reset",
    "lint": "eslint .",
    "test": "testcafe all tests/"
  },
  "dependencies": {
    "babel-core": "6.x.x",
    "babel-preset-env": "0.x.x",
    "bellajs": "6.x.x",
    "koa": "1.x.x",
    "koa-hbs": "0.x.x",
    "promise-wtf": "1.x.x"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "testcafe": "latest"
  },
  "builder": {
    "directories": [
      "storage/cache",
      "storage/tmp"
    ],
    "cssDir": "assets/css",
    "jsDir": "assets/js",
    "imgDir": "assets/images",
    "fontDir": "assets/fonts",
    "distDir": "dist",
    "vendorDir": "vendor",
    "css": {
      "mdl": "https://code.getmdl.io/1.2.1/material.indigo-pink.min.css"
    },
    "javascript": {
      "bella": "https://cdn.rawgit.com/ndaidong/bella.js/master/dist/bella.min.js",
      "material": "https://storage.googleapis.com/code.getmdl.io/1.2.1/material.js",
      "doc": "https://raw.githubusercontent.com/ndaidong/doc/master/dist/doc.min.js"
    }
  }
}
