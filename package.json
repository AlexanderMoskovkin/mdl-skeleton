{
  "version": "0.7.4",
  "name": "mdl-skeleton",
  "homepage": "https://techpush.xyz",
  "description": "Material Design Lite - Skeleton for Node.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/mdl-skeleton"
  },
  "author": "@ndaidong",
  "license": "MIT",
  "engines": {
    "node": ">= 6.0"
  },
  "main": "./server.js",
  "scripts": {
    "setup": "node ./bin/setup",
    "start": "npm run setup && node server",
    "reset": "node ./bin/reset",
    "lint": "eslint ."
  },
  "dependencies": {
    "babel-core": "6.*.*",
    "babel-preset-es2015": "6.*.*",
    "bellajs": "5.*.*",
    "body-parser": "1.*.*",
    "cookie-parser": "1.*.*",
    "copy-dir": "0.*.*",
    "css-mqpacker": "5.*.*",
    "cssnano": "3.*.*",
    "device-detector": "0.*.*",
    "express": "4.*.*",
    "handlebars": "4.*.*",
    "helmet": "2.*.*",
    "imagemin": "5.*.*",
    "imagemin-mozjpeg": "6.*.*",
    "imagemin-pngquant": "5.*.*",
    "mkdirp": "0.*.*",
    "mongoose": "4.*.*",
    "morgan": "1.*.*",
    "postcss": "5.*.*",
    "postcss-cssnext": "2.*.*",
    "postcss-filter-plugins": "2.*.*",
    "postcss-mixins": "5.*.*",
    "postcss-nested": "1.*.*",
    "postcss-nesting": "2.*.*",
    "postcss-scss": "0.*.*",
    "precss": "1.*.*",
    "promise-wtf": "1.*.*",
    "recursive-readdir": "2.*.*",
    "robots.txt": "1.*.*",
    "rucksack-css": "0.*.*",
    "sanitize-html": "1.*.*",
    "serve-favicon": "2.*.*",
    "shift-codegen": "5.*.*",
    "shift-parser": "5.*.*",
    "svgo": "0.*.*"
  },
  "devDependencies": {
    "eslint": "latest",
    "eslint-config-ggc": "latest"
  },
  "builder": {
    "directories": [
      "storage/cache",
      "storage/tmp"
    ],
    "cssDir": "assets/css",
    "jsDir": "assets/js",
    "imgDir": "assets/images",
    "fontDir": "assets/fonts",
    "distDir": "dist",
    "vendorDir": "vendor",
    "css": {
      "mdl": "https://code.getmdl.io/1.2.1/material.indigo-pink.min.css"
    },
    "javascript": {
      "bella": "https://cdn.rawgit.com/ndaidong/bella.js/master/dist/bella.min.js",
      "material": "https://storage.googleapis.com/code.getmdl.io/1.2.1/material.js",
      "doc": "https://raw.githubusercontent.com/ndaidong/doc/master/dist/doc.min.js"
    }
  }
}
