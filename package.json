{
  "version": "0.5.01",
  "name": "mdl-skeleton",
  "homepage": "http://techpush.net",
  "description": "Material Design Lite - Skeleton for Node.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:ndaidong/mdl-skeleton"
  },
  "author": "Dong Nguyen <ndaidong@gmail.com> (https://twitter.com/ndaidong)",
  "license": "MIT",
  "engines": {
    "node": ">= 4.0"
  },
  "main": "./server.js",
  "scripts": {
    "start": "node server.js",
    "reset": "./node_modules/.bin/gulp reset",
    "setup": "./node_modules/.bin/gulp setup"
  },
  "dependencies": {
    "async": "latest",
    "babel-core": "latest",
    "babel-preset-es2015": "latest",
    "bellajs": "latest",
    "bluebird": "latest",
    "body-parser": "latest",
    "cookie-parser": "latest",
    "copy-dir": "latest",
    "cssnano": "latest",
    "device-detector": "latest",
    "express": "latest",
    "handlebars": "latest",
    "helmet": "latest",
    "html-minifier": "latest",
    "mkdirp": "latest",
    "mongoose": "latest",
    "morgan": "latest",
    "postcss": "latest",
    "postcss-cssnext": "latest",
    "postcss-filter-plugins": "latest",
    "postcss-mixins": "latest",
    "postcss-nested": "latest",
    "postcss-nesting": "latest",
    "recursive-readdir": "latest",
    "request": "latest",
    "robots.txt": "latest",
    "sanitize-html": "latest",
    "serve-favicon": "latest",
    "shift-codegen": "latest",
    "shift-parser": "latest",
    "svgo": "latest"
  },
  "devDependencies": {
    "gulp": "latest"
  },
  "builder": {
    "directories": [
      "storage/cache",
      "storage/tmp"
    ],
    "cssDir": "assets/css",
    "jsDir": "assets/js",
    "imgDir": "assets/images",
    "fontDir": "assets/fonts",
    "distDir": "dist",
    "vendorDir": "vendor",
    "css": {
      "mdl": "https://code.getmdl.io/1.1.3/material.indigo-pink.min.css"
    },
    "javascript": {
      "bella": "https://cdn.rawgit.com/ndaidong/bella.js/master/dist/bella.min.js",
      "material": "https://storage.googleapis.com/code.getmdl.io/1.1.3/material.js",
      "vdom": "https://raw.githubusercontent.com/ndaidong/vdom/master/dist/vdom.min.js"
    }
  }
}
